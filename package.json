{
  "name": "sbom-scanner",
  "version": "2.0.2",
  "description": "Scan repo for projects and generate a bill-of-materials",
  "exports": "./dist/index.cjs",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "esbuild": "esbuild src/main.ts --bundle --platform=node --target=node16 --outfile=dist/index.cjs",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "scan": "npm run build && node --es-module-specifier-resolution=node lib/main.js",
    "scan-test": "npm run build && node --es-module-specifier-resolution=node lib/test.tmp.js",
    "dist": "rm -rf ./dist && npm run esbuild"
  },
  "license": "MIT",
  "author": "Ken Hill",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^5.0.3",
    "@actions/io": "^1.1.2",
    "@actions/tool-cache": "^2.0.1",
    "date-fns": "^2.29.3",
    "docker-file-parser": "^1.0.7",
    "node-fetch": "^3.2.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^28.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.15",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.36.2",
    "esbuild": "^0.15.13",
    "eslint": "^8.23.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.1",
    "jest": "^28.1.3",
    "jest-circus": "^29.0.2",
    "js-yaml": "^4.1.0",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  }
}
